{
  "name": "vish-hooks",
  "version": "0.0.0",
  "description": "A collection of 10+ hooks for state and UI management",
  "private": false,
  "homepage": "",
  "license": "MIT",
  "author": "Prince Vishwakarma <princevworld@gmail.com>",
  "keywords": [
    "frontend",
    "hooks",
    "library",
    "next",
    "nextjs",
    "react",
    "react-hooks",
    "state"
  ],
  "sideEffects": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "repository": {
    "url": "https://github.com/princevish/vish-hooks.git",
    "type": "git",
    "directory": "packages/@vish"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48"
  },
  "scripts": {
    "publish": "npm publish --access=public",
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  }
}
